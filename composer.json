{
  "name": "kudashevs/benchmark-php",
  "description": "A benchmark application with a set of benchmarks for PHP benchmarking.",
  "type": "library",
  "keywords": ["php", "benchmark", "benchmarking"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Kudashev",
      "email": "kudashevs@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.4 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "BenchmarkPHP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenchmarkPHP\\Tests\\": "tests/"
    }
  },
  "bin": [
    "benchmark-php"
  ]
}
